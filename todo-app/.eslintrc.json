{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": ["error", "always"],
        "no-unused-vars": ["warn"],
        "quotes": ["error", "double"]
    },
    "eslintignore": [
        "node_modules",
        "build",
        "dist",
        "webconfig.config.js"
    ]
}
